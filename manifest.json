{
	"name": "Copy Link Address",
	"manifest_version": 3,
	"version": "0.6.5",
	"description": "Copy link address without right-clicking. Just hover the link and hit Ctrl-C / Cmd-C!",
	"content_scripts": [
		{
			"matches": [
				"<all_urls>"
			],
			"css": [
				"css/notyf.min.css"
			],
			"js": [

				"js/notyf.min.js",
				"copylinkaddress.js"
			]
		}
	],
	"icons": {
		"48": "/icons/icon48.png",
		"128": "/icons/icon128.png"
	}
}
